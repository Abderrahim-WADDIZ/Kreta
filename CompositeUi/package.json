  "scripts": {
    "start": "babel-node scripts/start.js",
    "build": "babel-node scripts/build.js",
    "test": "babel-node scripts/test.js --env=jsdom"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/src/__mocks__/localStorage.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|config|etc|node_modules|public|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.(scss|css)$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|scss|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.js$"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom"
    ],
    "moduleNameMapper": {
      "\\.scss$": "<rootDir>/src/__mocks__/styleMock.js"
    }
  },
  "babel": {
    "presets": [
      "stage-2",
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-decorators-legacy",
      "./config/babelRelayPlugin"
    ]
  },
  "dependencies": {
    "classnames": "^2.2",
    "draft-js": "^0.10",
    "draftjs-to-html": "^0.6.3",
    "history": "^4.6.0",
    "lin3s-css-grid": "^0.3.0",
    "lodash": "^4.17",
    "lodash.debounce": "^4.0.8",
    "mousetrap": "^1.6.0",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-draft-wysiwyg": "^1.7.6",
    "react-redux": "^5.0.3",
    "react-relay": "^0.9.3",
    "react-router": "^2.0.0",
    "react-router-redux": "^2.1.0",
    "redux": "^3.6.0",
    "redux-form": "^6.2.0",
    "redux-logger": "^2.4.0",
    "redux-thunk": "^1.0.3"
  },
  "devDependencies": {
    "autoprefixer": "6.7.5",
    "babel-cli": "^6.18.0",
    "babel-core": "6.23.1",
    "babel-eslint": "7.1.1",
    "babel-jest": "18.0.0",
    "babel-loader": "7.0.0-alpha.3",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-app": "^2.1.1",
    "babel-preset-stage-2": "^6.18.0",
    "babel-relay-plugin": "^0.9.3",
    "babel-runtime": "^6.20.0",
    "case-sensitive-paths-webpack-plugin": "^1.1.4",
    "chalk": "^1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "cross-spawn": "4.0.2",
    "css-loader": "0.26.2",
    "detect-port": "1.1.0",
    "dotenv": "2.0.0",
    "enzyme": "^2.6.0",
    "eslint": "3.16.1",
    "eslint-config-react-app": "^0.6.1",
    "eslint-loader": "1.6.3",
    "eslint-plugin-flowtype": "2.21.0",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "^6.7.1",
    "exports-loader": "^0.6.2",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.10.1",
    "filesize": "^3.3.0",
    "fs-extra": "^1.0.0",
    "gzip-size": "^3.0.0",
    "html-webpack-plugin": "2.28.0",
    "http-proxy-middleware": "0.17.3",
    "imports-loader": "^0.6.5",
    "jest": "18.1.0",
    "node-sass": "^3.9.3",
    "object-assign": "4.1.1",
