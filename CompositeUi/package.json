    "project",
    "manager",
    "development",
    "tool",
    "bc",
    "composite-ui",
    "ui",
    "user-interface"
  ],
  "homepage": "http://kreta.io",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kreta/kreta.git"
