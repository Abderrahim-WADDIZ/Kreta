{
    "name": "kreta/notification-bundle",
    "description": "Kreta notification bundle",
    "keywords": [
        "project",
        "management",
        "development",
        "tool",
        "notification",
        "component"
    ],
    "homepage": "http://kreta.io",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Beñat Espiña Diaz",
            "email": "benatespina@gmail.com",
            "homepage": "http://benatespina.com"
        },
        {
            "name": "Gorka Laucirica Ibarra",
            "email": "gorka.lauzirika@gmail.com",
            "homepage": "http://gorkalaucirica.net"
        }
    ],
    "require": {
        "php":                ">=5.4",
        "kreta/core-bundle":  "^0.4.0",
        "kreta/notification": "^0.4.0"
    },
    "require-dev": {
        "phpspec/phpspec":    "^2.3"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/kreta/FOSOAuthServerBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/kreta/FOSUserBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/kreta/Finite"
        }
    ],
    "autoload": {
        "psr-4": {
            "Kreta\\Bundle\\NotificationBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kreta\\Bundle\\NotificationBundle\\spec\\": "spec/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.4-dev"
        }
    }
}
